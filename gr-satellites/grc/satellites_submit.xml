<?xml version="1.0"?>
<block>
  <name>Telemetry Forwarder</name>
  <key>satellites_submit</key>
  <category>[Satellites]/Misc</category>
  <import>import satellites</import>
  <make>satellites.submit($url, $noradID, $source, $longitude, $latitude, $tstamp)</make>
  <param>
    <name>URL</name>
    <key>url</key>
    <type>string</type>
  </param>
  <param>
    <name>NORAD ID</name>
    <key>noradID</key>
    <type>int</type>
  </param>
  <param>
    <name>Receiver callsign</name>
    <key>source</key>
    <type>string</type>
  </param>
    <param>
    <name>Latitude (N is +, S is -)</name>
    <key>latitude</key>
    <type>float</type>
  </param>
  <param>
    <name>Longitude (W is -, E is +)</name>
    <key>longitude</key>
    <type>float</type>
  </param>
  <param>
    <name>Start of recording UTC (blank for realtime)</name>
    <key>tstamp</key>
    <value></value>
    <type>string</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
</block>
