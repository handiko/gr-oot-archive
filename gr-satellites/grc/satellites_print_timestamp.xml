<?xml version="1.0"?>
<block>
  <name>Print timestamp</name>
  <key>satellites_print_timestamp</key>
  <category>[Satellites]/Misc</category>
  <import>import satellites</import>
  <make>satellites.print_timestamp($fmt)</make>

   <param>
    <name>Format</name>
    <key>fmt</key>
    <value>%Y-%m-%d %H:%M:%S</value>
    <type>string</type>
  </param>
  
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
