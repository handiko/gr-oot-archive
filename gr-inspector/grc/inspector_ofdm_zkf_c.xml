<?xml version="1.0"?>
<block>
  <name>OFDM Estimator</name>
  <key>inspector_ofdm_zkf_c</key>
  <category>[Inspector]/Analysis</category>
  <import>import inspector</import>
  <make>inspector.ofdm_zkf_c($samp_rate, $signal, $min_items, $alpha, $beta)</make>
  <callback>set_samp_rate($samp_rate)</callback>
  <param>
    <name>Samp rate</name>
    <key>samp_rate</key>
    <type>real</type>
  </param>
  <param>
    <name>Signal No</name>
    <key>signal</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>Min input items</name>
    <key>min_items</key>
    <value>7000</value>
    <type>int</type>
  </param>
  <param>
    <name>FFT lengths</name>
    <key>alpha</key>
    <value>[16, 32, 64, 128, 256, 512, 1024]</value>
    <type>int_vector</type>
  </param>
  <param>
    <name>CP lengths</name>
    <key>beta</key>
    <value>[4, 8, 16, 32]</value>
    <type>int_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>ofdm_out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
